		"@emotion/styled": "11.11.5",
		"@repo/ui": "workspace:*",
		"@vscode/codicons": "^0.0.35",
		"framer-motion": "^6",
		"next": "^14.0.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^5.1.0",
		"tslib": "^2.6.2",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.7.2",
