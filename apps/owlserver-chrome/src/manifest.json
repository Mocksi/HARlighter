{
    "manifest_version": 3,
    "name": "Owlserver by Mocksi",
    "description": "Owlserver records interactions and sends them to Mocksi",
    "version": "1.0",
    "action": {
      "default_popup": "./assets/default_popup.html",
      "default_icon": "./assets/owlserver.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["main.js"]
      }
    ],
    "host_permissions": [
      "*://*/*"
    ]
  }